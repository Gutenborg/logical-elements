{
  "name": "logical-elements",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  },
  "scripts": {
    "dev": "vite ./src/documentation",
    "build": "npm run build:le && npm run build:documentation",
    "build:le": "tsc --project tsconfig.json",
    "build:documentation": "vite build ./src/documentation --config vite.config.js",
    "preview": "vite preview ./src/documentation --config vite.config.js"
  },
  "workspaces": [
    "packages/components",
    "packages/core",
    "packages/documentation"
  ]
}
